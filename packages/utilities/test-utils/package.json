{
  "name": "@banyu/test-utils",
  "version": "3.0.1",
  "description": "A set of utilities for react testing",
  "keywords": [
    "test-utils"
  ],
  "author": "Muhammad Amien <muhammadamien23@gmail.com>",
  "homepage": "#",
  "license": "MIT",
  "main": "dist/index.js",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muhamien/jala-design.git",
    "directory": "packages/utilities/test-utils"
  },
  "bugs": {
    "url": "https://github.com/muhamien/jala-design/issues"
  },
  "scripts": {
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "dev": "yarn build:fast -- --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "clean-package": "../../../clean-package.config.json",
  "tsup": {
    "clean": true,
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ]
  },
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  }
}
