{
  "name": "@banyu/react-utils",
  "version": "2.2.0",
  "description": "A set of utilities for react on client side",
  "keywords": [
    "react-utils"
  ],
  "author": "Muhammad Amien <muhammadamien23@gmail.com>",
  "homepage": "#",
  "license": "MIT",
  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muhamien/jala-design.git",
    "directory": "packages/utilities/react-utils"
  },
  "bugs": {
    "url": "https://github.com/muhamien/jala-design/issues"
  },
  "scripts": {
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "dev": "yarn build:fast -- --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "dependencies": {
    "@banyu/shared-utils": "workspace: *",
    "@banyu/react-rsc-utils": "workspace: *"
  },
  "devDependencies": {
    "clean-package": "2.2.0",
    "react": "^18.0.0"
  },
  "clean-package": "../../../clean-package.config.json"
}
